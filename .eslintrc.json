{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": "standard",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "fetch": true,
      "mapboxgl": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "indent": [
        2,
        2
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }